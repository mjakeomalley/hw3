<h1>New Post</h1>

<%= form_with :model => @post do |form| %>
  <h2>
    <%= form.label "title" %>
    <%= form.text_field "title" %>
  </h2>

  <h3>
    <%= form.label "posted_on" %>
    <%= form.date_field "posted_on" %>
  </h3>

  <p>
    <%= form.label "description" %>
    <%= form.text_field "description" %>
  </p>
  
  <%= form.text_field "place_id" %>
  <%= form.submit %>
<% end %>